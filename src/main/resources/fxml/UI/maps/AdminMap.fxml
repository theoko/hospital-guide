<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTabPane?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import net.kurobako.gesturefx.GesturePane?>

<?import com.jfoenix.controls.JFXTextField?>
<AnchorPane stylesheets="@../../../css/colorScheme.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.maps.AdminMapController">
    <JFXTabPane tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <Tab text="Map Editor">
            <AnchorPane>
                <GesturePane fx:id="gesMap" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <AnchorPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                        <ImageView fx:id="imgMap" />
                        <Pane fx:id="panMap" />
                    </AnchorPane>
                </GesturePane>
                <VBox spacing="25">
                    <padding>
                        <Insets bottom="25" left="25" right="25" top="25" />
                    </padding>
                    <HBox alignment="CENTER_LEFT" spacing="10">
                        <JFXButton fx:id="btnFloor4" text="4" styleClass="animated-option-button" onMouseClicked="#btnFloor4_Click"/>
                    </HBox>
                    <HBox alignment="CENTER_LEFT" spacing="10">
                        <JFXButton fx:id="btnFloor3" onMouseClicked="#btnFloor3_Click" styleClass="animated-option-button" text="3" />
                    </HBox>
                    <HBox alignment="CENTER_LEFT" spacing="10">
                        <JFXButton fx:id="btnFloor2" onMouseClicked="#btnFloor2_Click" styleClass="animated-option-button" text="2" />
                    </HBox>
                    <HBox alignment="CENTER_LEFT" spacing="10">
                        <JFXButton fx:id="btnFloor1" onMouseClicked="#btnFloor1_Click" styleClass="animated-option-button" text="1" />
                    </HBox>
                    <HBox alignment="CENTER_LEFT" spacing="10">
                        <JFXButton fx:id="btnFloorG" onMouseClicked="#btnFloorG_Click" styleClass="animated-option-button" text="G" />
                    </HBox>
                    <HBox alignment="CENTER_LEFT" spacing="10">
                        <JFXButton fx:id="btnFloorL1" onMouseClicked="#btnFloorL1_Click" styleClass="animated-option-button" text="L1" />
                    </HBox>
                    <HBox alignment="CENTER_LEFT" spacing="10">
                        <JFXButton fx:id="btnFloorL2" onMouseClicked="#btnFloorL2_Click" styleClass="animated-option-button" text="L2" />
                    </HBox>
                </VBox>
                <AnchorPane AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="200.0">
                    <VBox fx:id="vboxDock" prefHeight="468.0" prefWidth="60.0"
                          style="-fx-background-color: #022D5A; -fx-background-radius: 30;"
                          AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                          AnchorPane.topAnchor="0.0"/>
                    <JFXButton fx:id="btnLogOut" contentDisplay="CENTER" onMouseClicked="#btnReturn_Click"
                               prefHeight="60.0" prefWidth="60.0" textOverrun="CLIP">
                        <graphic>
                            <ImageView fx:id="imgLogOut" fitHeight="30.0" fitWidth="30.0" pickOnBounds="true"
                                       preserveRatio="true"/>
                        </graphic>
                        <font>
                            <Font size="1.0"/>
                        </font>
                        <padding>
                            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                        </padding>
                    </JFXButton>
                </AnchorPane>
                <AnchorPane style="-fx-background-color: white;" fx:id="roomBooking">
                    <fx:include source="../booking/RoomBookingWindow.fxml" />
                </AnchorPane>
                <AnchorPane style="-fx-background-color: white;" fx:id="UserD">
                    <fx:include source="../user/CreateUser.fxml" />
                </AnchorPane>
                <AnchorPane fx:id="Algo">
                    <fx:include source="../settings/SettingsScreen.fxml" />
                </AnchorPane>
                <AnchorPane style="-fx-background-color: white;" fx:id="Clean">
                    <fx:include source="../sanitation/AdminSanitation.fxml" />
                </AnchorPane>
                <AnchorPane style="-fx-background-color: white;" fx:id="TabEdit">
                    <fx:include source="TableEditor.fxml" />
                </AnchorPane>
                <AnchorPane AnchorPane.topAnchor="15" AnchorPane.rightAnchor="15">
                    <fx:include source="../weather/Weather.fxml"/>
                </AnchorPane>
                <JFXTextField fx:id="search" prefWidth="450" prefHeight="34" promptText=" Search"  alignment="CENTER" style=" -fx-font-size: 18px; -fx-font-weight: bold; -fx-font-family: fantasy; -fx-text-fill: #022D5A; -fx-background-color: white" />
            </AnchorPane>
        </Tab>
    </JFXTabPane>
</AnchorPane>
