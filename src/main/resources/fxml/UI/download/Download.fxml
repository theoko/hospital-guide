<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="700.0" prefWidth="1200.0" stylesheets="@../../../css/colorScheme.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.download.DownloadController">
   <children>
      <HBox alignment="CENTER" layoutY="95.0" prefHeight="100.0" prefWidth="200.0" spacing="40.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <JFXButton fx:id="buttonYes" onMouseClicked="#clickButtonYes" text="Yes" textFill="WHITE" />
            <JFXButton fx:id="buttonNo" onMouseClicked="#clickButtonNo" stylesheets="@../../../css/colorScheme.css" text="No" textFill="WHITE" />
         </children>
      </HBox>
      <Label alignment="TOP_CENTER" layoutY="50.0" stylesheets="@../../../css/colorScheme.css" text="Do you want to download?" textAlignment="CENTER" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <font>
            <Font size="27.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
